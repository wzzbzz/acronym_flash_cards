{% extends 'base.html.twig' %}

{% block title %}Acronym{% endblock %}

{% block body %}
    <h1>Acronym</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ acronym.id }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ acronym.code }}</td>
            </tr>
            <tr>
                <th>Meaning</th>
                <td>{{ acronym.meaning }}</td>
            </tr>
            <tr>
                <th>WikipediaUrl</th>
                <td>{{ acronym.wikipediaUrl }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_acronym_index') }}">back to list</a>

    <a href="{{ path('app_acronym_edit', {'id': acronym.id}) }}">edit</a>

    {{ include('acronym/_delete_form.html.twig') }}
{% endblock %}
